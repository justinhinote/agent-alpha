{
  "name": "agent-alpha",
  "version": "0.1.0",
  "description": "CLI-first TypeScript foundation for Agent Alpha",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent-alpha": "dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "lint": "eslint .",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run",
    "start": "node dist/cli.js"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.30.2",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^22.14.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript-eslint": "^8.44.0",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  }
}
